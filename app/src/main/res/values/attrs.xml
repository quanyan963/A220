<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="Roman">
        <attr name="text_type" format="enum">
            <enum name="one" value="0"/>
            <enum name="two" value="1"/>
            <enum name="three" value="2"/>
        </attr>
    </declare-styleable>
    <declare-styleable name="LineChart">
        <attr name="text_color" format="color|reference"/>
        <attr name="line_color" format="color|reference"/>
        <attr name="circle_color" format="color|reference"/>
        <attr name="canvas_color" format="color|reference"/>
        <attr name="text_size" format="dimension|reference"/>
    </declare-styleable>
</resources>